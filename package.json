    "js-yaml-loader": "^1.2.2",
    "jsdiff": "^1.1.1",
    "jsdom-global": "^3.0.2",
    "jsonpath-plus": "^6.0.1",
    "jsrsasign": "^10.2.0",
    "jszip": "^3.2.2",
    "lodash": "^4.17.21",
    "marked": "^2.0.0",
    "node-sass": "^4.12.0",
    "nuxt": "2.14.6",
    "papaparse": "^5.3.0",
    "portal-vue": "^2.1.5",
