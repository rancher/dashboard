    "@popperjs/core": "^2.4.0",
    "ansi_up": "^5.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-env": "^3.2.6",
    "cookie": "^0.4.0",
    "cookie-universal-nuxt": "^2.0.17",
    "cron-validator": "^1.2.0",
    "cronstrue": "^1.92.0",
    "cross-env": "^6.0.3",
    "d3": "^5.12.0",
    "d3-selection": "^1.3.0",
    "dagre-d3": "^0.6.3",
    "dayjs": "^1.8.16",
    "diff2html": "^2.11.2",
