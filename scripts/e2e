#!/usr/bin/env bash
ID="$(echo ${TEST_RUN_ID:-`date +%s` $GREP_TAGS } | tr '[:upper:]' '[:lower:]' | tr ' ' '-')"
echo "$ID"

CYPRESS_coverage=true CYPRESS_API_URL='http://139.59.134.103:1234' cy2 run --browser --group $GREP_TAGS chrome --record --key rancher-dashboard --parallel --ci-build-id "$ID"
