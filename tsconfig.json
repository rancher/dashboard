{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM",
      "dom.iterable",
      "scripthost"
    ],
    "paths": {
      "@/*": [
        "shell/*"
      ],
      "@shell/*": [
        "shell/*"
      ],
      "@rc/*": [
        "shell/rc/*"
      ]
    },
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "baseUrl": ".",
    "rootDir": ".",
    "types": [
      "webpack-env",
      "node",
      "jest",
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "jsx": "preserve",
    "importHelpers": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
  },
  "exclude": [
    "node_modules",
    "dist",
    "dist-pkg",
    "cypress",
    "shell/scripts",
    "cypress",
    "./cypress.config.ts",
    "docusaurus",
    "scripts/standalone",
    "**/*.spec.ts"
  ]
}